        <record id="group_laundry" model="res.groups">
            <field name="name">Laundry</field>
            <field name="category_id" ref="base.module_category_hidden"/>
        </record>

        <record id="group_pickup_drop" model="res.groups">
            <field name="name">Pickup &amp; Drop</field>
            <field name="category_id" ref="base.module_category_hidden"/>
        </record>

        <record id="group_house_keeping" model="res.groups">
            <field name="name">House keeping</field>
            <field name="category_id" ref="base.module_category_hidden"/>
        </record>

        <record model="ir.rule" id="housing_rule">
            <field name="name">housekepping dashboard</field>
            <field name="model_id" ref="base.model_res_partner"/>
            <field name="domain_force">[(1,'=',1)]</field>
            <field name="groups" eval="[(4, ref('group_house_keeping'))]"/>
            <field name="perm_read" eval="True"/>
        </record>

        <record model="res.groups" id="group_multi_branches">
            <field name="name">Multi Branches</field>
            <field name="category_id" ref="base.module_category_usability" />
        </record>

        <record id="aspl_hotel.module_category_hotel" model="ir.module.category">
            <field name="name">Hotel</field>
            <field name="description">Helps you handle Hotel.</field>
            <field name="sequence">1</field>
        </record>

        <record id="group_hotel_house_keeper" model="res.groups">
            <field name="name">House Keeper</field>
            <field name="category_id" ref="aspl_hotel.module_category_hotel"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>

        <record id="group_hotel_house_keeper_manager" model="res.groups">
            <field name="name">House Keeper Manager</field>
            <field name="implied_ids" eval="[(4, ref('group_hotel_house_keeper'))]"/>
            <field name="category_id" ref="aspl_hotel.module_category_hotel"/>
            <field name="users" eval="[(4, ref('base.group_user'))]"/>
        </record>

        <record id="group_vahicle_driver" model="res.groups">
            <field name="name">Driver</field>
            <field name="category_id" ref="aspl_hotel.module_category_hotel"/>
            <field name="users" eval="[(4, ref('base.group_user'))]"/>
        </record>

        <record id="group_hotel_manager" model="res.groups">
